{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "array",
  "items": {
    "type": "object",
    "required": ["id", "url", "width", "height"],
    "properties": {
      "id": {
        "type": "string"
      },
      "url": {
        "type": "string",
        "format": "uri"
      },
      "width": {
        "type": "integer",
        "minimum": 1
      },
      "height": {
        "type": "integer",
        "minimum": 1
      }
    }
  }
}
